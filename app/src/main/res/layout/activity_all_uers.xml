<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ltd.akhbod.flychats.AllUersActivity">

    <include layout="@layout/appbarlayout"
        android:id="@+id/allusers_applayout"></include>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/allusers_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/allusers_applayout"></android.support.v7.widget.RecyclerView>

</RelativeLayout>
